        %li.offline{"data-user-id" => "4"}
          %a{:href => "#"}
            .media
              .pull-left
                %img.img-circle{:src => "assets/people/110/woman-6.jpg", :width => "40"}/
              .media-body
                .contact-name Michelle S.
                %small Offline
    %script#chat-window-template{:type => "text/x-handlebars-template"}
      .panel.panel-default
        .panel-heading{"data-target" => "#chat-bill", "data-toggle" => "chat-collapse"}
          %a.close{:href => "#"}
            %i.fa.fa-times
          %a{:href => "#"}
            %img.pull-left{:src => "{{ user_image }}", :width => "40"}
              %span.contact-name {{user}}
        #chat-bill.panel-body
          .media
            .pull-left
              %img.img-circle{:alt => "people", :src => "{{ user_image }}", :width => "25"}/
            .media-body
              %span.message Feeling Groovy?
          .media.right
            .pull-right
              %img.img-circle{:alt => "people", :src => "{{ user_image }}", :width => "25"}/
            .media-body
              %span.message Yep.
          .media
            .pull-left
              %img.img-circle{:alt => "people", :src => "{{ user_image }}", :width => "25"}/
            .media-body
              %span.message This chat window looks amazing.
          .media.right
            .pull-right
              %img.img-circle{:alt => "people", :src => "{{ user_image }}", :width => "25"}/
            .media-body
              %span.message Thanks!
