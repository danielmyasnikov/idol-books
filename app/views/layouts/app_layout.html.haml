    / Fixed navbar
    .navbar.navbar-main.navbar-default{:role => "navigation"}
      .container-fluid
        %a#toggle-sidebar-menu.visible-xs{"data-toggle" => "sidebar-menu", :href => "#"}
          %i.fa.fa-bars.fa-2x
        
        %ul.nav.navbar-nav.navbar-user
          / User
          %li.dropdown
            %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
              %img.img-circle{:alt => "Bill", :src => "/assets/people/50/guy-5.jpg", :width => "35"}/
              Bill
              %span.caret
            %ul.dropdown-menu{:role => "menu"}
              %li
                %a{:href => "profile.html"} Profile
              %li
                %a{:href => "messages.html"} Messages
              %li
                %a{:href => "login.html"} Logout
        %ul.nav.navbar-nav.hidden-xs
          / Search
          .input-group.pull-left
            %span.input-group-addon
              %i.icon-search
            %input.form-control{:placeholder => "Search a friend", :type => "text"}
          .pull-left.visible-md.visible-lg{"data-placement" => "bottom", "data-toggle" => "tooltip", :title => "A few Color Examples. Download includes CSS Files for all color examples & the tools to Generate any Color combination. This Color-Switcher is for previewing purposes only."}
    
    .sidebar.right
      .chat-search
        %input.form-control{:placeholder => "Search", :type => "text"}/
      %ul.chat-filter.nav.nav-pills
        %li.active
          %a{"data-target" => "li", :href => "#"} All
        %li
          %a{"data-target" => ".online", :href => "#"} Online
        %li
          %a{"data-target" => ".offline", :href => "#"} Offline
      %ul.chat-contacts
        %li.online{"data-user-id" => "1"}
          %a{:href => "#"}
            .media
              .pull-left
                %span.status
                %img.img-circle{:src => "assets/people/110/guy-6.jpg", :width => "40"}/
              .media-body
                .contact-name Jonathan S.
                %small "Free Today"
        %li.online.away{"data-user-id" => "2"}
          %a{:href => "#"}
            .media
              .pull-left
                %span.status
                %img.img-circle{:src => "assets/people/110/woman-5.jpg", :width => "40"}/
              .media-body
                .contact-name Mary A.
                %small "Feeling Groovy"
        %li.online{"data-user-id" => "3"}
          %a{:href => "#"}
            .media
              .pull-left
                %span.status
                %img.img-circle{:src => "assets/people/110/guy-3.jpg", :width => "40"}/
              .media-body
